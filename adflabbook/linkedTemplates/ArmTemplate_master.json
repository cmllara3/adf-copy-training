{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adflabbook"
		},
		"dllabbook_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dllabbook'"
		},
		"salabbook_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'salabbook'"
		},
		"sqldatabaselabbook_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldatabaselabbook'"
		},
		"dllabbook_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dllabbook.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adflabbook_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dllabbook_accountKey": {
						"value": "[parameters('dllabbook_accountKey')]"
					},
					"salabbook_connectionString": {
						"value": "[parameters('salabbook_connectionString')]"
					},
					"sqldatabaselabbook_connectionString": {
						"value": "[parameters('sqldatabaselabbook_connectionString')]"
					},
					"dllabbook_properties_typeProperties_url": {
						"value": "[parameters('dllabbook_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}